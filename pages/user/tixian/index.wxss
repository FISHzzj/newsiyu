/* pages/user/tixian/index.wxss */
.tixian{
  font-size: 30rpx;

}
.tixian .tixian_yue{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  padding: 20rpx;
  box-sizing: border-box;
 
}

.tixian .tixian_yue>view{
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  /* background-color: #999999; */
  width: 100%;
  border-radius: 10rpx;
  color: #eeeeee;

}
.tixian_btn{
  width: 95%;
  margin: 20rpx 20rpx;
  background-color: #d83e3e;
  color: #ffffff;
  text-align: center;
  padding: 20rpx 0;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.datetime{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding: 20rpx 20rpx;
  box-sizing: border-box;

}
.qudao {
  margin: 30rpx 30rpx;
}
.qudao .qudaotop{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  color: #999999;
  background-color: #eeeeee;
  margin-bottom: 10rpx;
  font-size: 32rpx;
}
